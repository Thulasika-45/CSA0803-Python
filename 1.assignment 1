1.iAPPEND
my_list = [1, 2, 3]
my_list.append(4)
print(my_list)  
# Output: [1, 2, 3, 4]
ii.COUNT
my_list = [1, 2, 2, 3, 2]
count = my_list.count(2)
print(count)  
# Output: 3
iii.INDEX
my_list = [10, 20, 30, 40, 50]
index = my_list.index(30)
print(index)  
# Output: 2
iv.INSERT
my_list = [1, 2, 4, 5]
my_list.insert(2, 3)
print(my_list)  
# Output: [1, 2, 3, 4, 5]
v.POP
my_list = [1, 2, 3, 4, 5]
popped_element = my_list.pop()
print(popped_element)  
# Output: 5
vi.REMOVE 
my_list = [1, 2, 3, 2, 4]
my_list.remove(2)
print(my_list)  
# Output: [1, 3, 2, 4]
vii.REVERSE
my_list = [1, 2, 3, 4, 5]
my_list.reverse()
print(my_list) 
# Output: [5, 4, 3, 2, 1]
viii.SORT
my_list = [4, 1, 3, 2, 5]
my_list.sort()
print(my_list) 
# Output: [1, 2, 3, 4, 5]
ix.EXTEND
list1 = [1, 2, 3]
list2 = [4, 5, 6]
list1.extend(list2)
print(list1) 
# Output: [1, 2, 3, 4, 5, 6]
